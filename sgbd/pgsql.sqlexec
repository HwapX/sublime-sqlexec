{
    "sql_exec": {
        "queries": {
            "desc" : "SELECT c.relname FROM pg_catalog.pg_class AS c INNER JOIN pg_catalog.pg_namespace AS n ON n.oid = c.relnamespace WHERE relkind = 'r' AND n.nspname = 'public' ORDER BY n.nspname = current_schema() DESC, pg_catalog.pg_table_is_visible(c.oid) DESC, n.nspname, c.relname;",
            "desc table": "SELECT * FROM information_schema.columns WHERE table_schema = 'public' AND table_name = '%s'",
            "show records": "select * from %s limit 100"
        }
    }
}
